{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "settings": {
        "react": {
            "version": "none"
        }
    },
    "extends": "airbnb",
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": ["off", "windows"],
        "no-restricted-syntax": "off",
        "no-console": "error",
        "no-var": "error"
    }
}
